
 <% if(userId == 'admin'){%>
    <a href="/board/write" style="position:absolute; left:45%">글작성</a><br>
     <%}else{%>
     
     <%}%>
     
<title><%=title%></title>
<table class="table table-hover">
    <tr>
        <td>글번호</td>
        <td>작성자</td>
        <td>제목</td>
        <td>조회수</td>
        <td>등록일</td>
    </tr>
    <%
    for(var i = (page * page_num) - page_num; i < (page * page_num); i++) {
    if(i > length){
        i++;
    }else{
        var data = rows[i]
    %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= data.name %></td>
        <td><a href="/board/view/<%= data.idx %>"><%= data.title %></a></td>
        <td><%= data.hit %></td>
       
        <td><%= data.regdate %></td>
    </tr>
    <% }
    }
    %>
    <tr style="position:absolute; left:45%">
        <td colspan="6" >
            <%
            for(var j = 0; j < rows.length / page_num; j++){
            %>
            [<a href="/board/page/<%= j + 1 %>"><%= j + 1 %></a>]
            <%
            }
            %>
        </td>
    </tr>
</table>