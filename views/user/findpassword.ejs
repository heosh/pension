

<form class="login-form" modelAttribute="user" method="post" action="findpassword" >
    
    <div>

        <h3 class="join_title">

            <label for="id">아이디</label>

        </h3>

        <input type="text" class="form-control" path="userId"
            id="userId" name="userId" />
            
        


    </div>
    <div>

        <h3 class="join_title">

            <label for="pswd1">email</label>

        </h3>

        <input type="text" path="email" id='email' name="email"
            class="form-control" maxlength="30" size='10' />
        



    </div>

    
    <div class="btn_area">
   
        <button type="submit" id="btnJoin">

            <span>비밀번호변경</span>

        </button>

    </div>

</form>
